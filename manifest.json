{
  "manifest_version": 2,
  "name": "YouTube Mute Words",
  "version": "1.0",
  "description": "Hides Youtube videos with specified words in title",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "gui.html",
    "default_icon": {
      "16": "resources/icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["js/main.js"]
    }
  ]
}
